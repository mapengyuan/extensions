{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "color-palette",
  "title": "Color Palette",
  "description": "Your Global Color Palettes",
  "icon": "Logo_ColorPalette.png",
  "author": "Forest1037",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "color-palette",
      "title": "Color Palette",
      "subtitle": "Your Global Color Palettes",
      "description": "Choose palette from built-in popular color palettes; Preview the appearance, name and HSL value of each color; Copy the color value (Hex or RGB or HSL) to your clipboard.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@adobe/leonardo-contrast-colors": "^1.0.0-alpha.13",
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.5.2",
    "color-scheme": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
